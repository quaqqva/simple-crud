{
  "JwtSettings": {
    "Issuer": "http://backend",
    "Audience": "http://frontend",
    "AccessTokenExpireMinutes": 2,
    "RefreshTokenExpireDays": 30
  },
  "ConnectionStrings": {
    "DefaultConnection": "server=db;port=3306;user=root;password={password};database=typography"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
